<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_LEFT" maxHeight="Infinity" maxWidth="Infinity" prefWidth="400.0" spacing="10.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="3.0" left="5.0" right="5.0" top="3.0" />
    </padding>
    <children>
        <ImageView fx:id="picture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="dialog" minHeight="-Infinity" text="Label" wrapText="true" maxWidth="300.0" style="-fx-background-color: #e1f5fe; -fx-background-radius: 10; -fx-padding: 10; -fx-font-size: 13px;" />
    </children>
</fx:root>
